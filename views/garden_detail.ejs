				var title = $('.title').val();
				console.log(title);
				console.log(x);
				var map = new google.maps.Map(document.getElementById('map_canvas'), {
					center: {lat: x, lng: y},
					zoom: 16
				});
				var marker = new google.maps.Marker({
					position: {lat: x, lng: y},
					map: map,
					title: title
				});
			}
		</script>
