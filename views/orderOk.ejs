        <div id="main"> 
			
			<!-----회원가입--->
			
		<div id="order">
			<div class="order_text">
				<tt class="name">주문완료</tt><br>
				<tt class="name2">키친가든 상품을 주문해주셔서 감사합니다!</tt>
			</div>
			
			<div class="order_info">
				<tt class="order_info_text">상품정보</tt>
			</div>
			<div class="order_info2">
				<p class="order_info_text2">상품정보</p>
				<p class="order_info_text3">상품개수</p>
				<p class="order_info_text3">상품가격</p>
			</div>
			
			<div class="info">
				<%
						for(var i=0; i < goods.length; i++){
					%>
					<div class="border">
						
						<p class="image"><img src="/images/store_sum_img_<%=goods[i].g_id%>.png" style="width: 150px; height: 150px; position: relative; left:50px;"></p>
						<div class="info_name">
							<p class="info_name_text"><%=goods[i].name%><input type="hidden" value="<%=goods[i].g_id%>" name="g_id"></p>
						</div>
						<div class="info_price">
							<p class="info_price_name"><%=goods[i].quantity%><input type="hidden" value="<%=goods[i].quantity%>" name="quantity"></p>
						</div>
						<div class="info_price">
							<p class="info_price_name"><%=goods[i].total.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>원</p>
						</div>
					</div>
						
					<%
						}
					%>
			</div>
			
			
			<div class="orderer">
				<tt class="order_info_text">주문자 정보</tt>
			</div>
			
			<table class="orderer_name">
				<tr>
					<td class="name3">
						보내는 분*
					</td>
					<td class="finish">
						<%=user_info.name%>
					</td>
				</tr>
				
				
				<tr>
					<td class="phone">
						휴대폰*
					</td>
						<td class="finish">
						<%=user_info.phone%>
					</td>
				</tr>
				
				<tr>
					<td class="phone">
						이메일*
					</td>
						<td class="finish">
							<%=user_info.email%>
					</td>
				</tr>
				
			</table>
			
			
					<div class="orderer">
				<tt class="order_info_text">수령인 정보</tt>
			</div>
			
			<table class="orderer_name">
				<tr>
					<td class="name3">
						수령인*
					</td>
						<td class="finish">
							<%=receiver.name%>
					</td>
				</tr>
				
				
				<tr>
					<td class="phone">
						휴대폰*
					</td>
						<td class="finish">
							<%=receiver.phone%>
					</td>
				</tr>
				
				<tr>
					<td class="name31">
						주소*
					</td>
					<td class="finish">
						<%=receiver.address%>
					</td>
				</tr>
				<tr>
					<td class="phone1">
						배송 요청사항
					</td>
						<td class="finish">
							<%=receiver.memo%>
					</td>
				</tr>
			</table>
				
			
			
			<div class="orderer">
				<tt class="order_info_text">결제정보</tt>
			</div>
			
		
				<table class="orderer_name">
				<tr>
					<td class="name3">
						결제수단
					</td>
					<td class="finish">
						<%=order_info.pay%>
					</td>
				</tr>
				
				
				<tr>
					<td class="phone">
						적립금 사용*
					</td>
						<td class="finish">
							<%=order_info.usePoint.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>원
					</td>
				</tr>
				
				<tr>
					<td class="phone">
						총 결제금액*
					</td>
					<td class="finish">
						<%=order_info.total.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",")%>원
					</td>
				</tr>
			</table>
				
			
			<div class="btn">
				<a href="/">
			<input type="button" value="홈으로" class="pay_btn">
				</a>
			</div>
			
		</div>
			
			
					
		
			
			
  


